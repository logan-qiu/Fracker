generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  username String @db.VarChar(255) @unique
  password String @db.VarChar(255)
  email    String @db.VarChar(255) @unique
}